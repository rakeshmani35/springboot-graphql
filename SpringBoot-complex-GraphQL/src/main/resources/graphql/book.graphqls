type Mutation{
  createBook(book:BookInput):BookOutput
}

type Query{
  allBooks:[BookOutput]
  getBookById(bookId:Int):BookOutput
  getBook(book:BookInput):[BookOutput]
  getBookByIdAndAuthor(bookId:Int, author:String):BookOutput
}

type BookOutput{
  id:ID!
  title:String
  desc:String
  price:Float
  author:[AuthorOutput]
  pages:Int
}


input BookInput{
    title:String
    desc:String
    author:String
    price:Float
    pages:Int
}